extends layout
block content
  each key in Object.keys(file)
    if key === "folder"
      p.d-flex.gap-2
        span.fw-bold Folder Name:
        span #{ file[key].name }
    if key === "details"
      each detail in Object.keys(file[key])
        p.d-flex.gap-2
          span.fw-bold File #{ detail }:
          span #{ file[key][detail] }
    - const notDetailsOrFolder = key !== 'folder' && key !== 'details';
    if notDetailsOrFolder
      p.d-flex.gap-2
        span.fw-bold File #{ key }:
        span #{ file[key] }
